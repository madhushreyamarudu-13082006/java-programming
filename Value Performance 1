import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner ms = new Scanner(System.in);
        int a = ms.nextInt();
        float b = ms.nextFloat();
        System.out.println(a);
        System.out.printf("%.2f",b);
    }
}
